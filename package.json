    "nodemon": "^1.18.9",
    "pg": "^7.8.0",
    "prettier": "^1.16.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
